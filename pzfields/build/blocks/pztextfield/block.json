{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "pz/pz-textfield",
  "version": "0.1.0",
  "title": "PZ MUI Text Field",
  "category": "widgets",
  "description": "Show a list of all link resources.",
  "supports": {
    "html": false
  },
  "textdomain": "pzfields",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "prompt": {
      "type": "string",
      "default": "Value"
    },
    "addURL": {
      "type": "string",
      "default": "/edit-project/"
    },
    "editURL": {
      "type": "string",
      "default": "/edit-project/"
    },
    "siteURL": {
      "type": "string"
    },
    "taskListURL": {
      "type": "string",
      "default": "/tasks/"
    },
    "backgroundColor": {
      "type": "string"
    }
  }
}