{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "pz/pzprojectgrid",
  "version": "0.1.0",
  "title": "PZ Project Grid",
  "category": "widgets",
  "description": "Show a list of all projects.",
  "supports": {
    "html": false
  },
  "textdomain": "pzprojectgrid",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "myheader": {
      "type": "string"
    },
    "addURL": {
      "type": "string",
      "default": "/edit-project/"
    },
    "editURL": {
      "type": "string",
      "default": "/edit-project/"
    },
    "siteURL": {
      "type": "string"
    },
    "taskListURL": {
      "type": "string",
      "default": "/tasks/"
    },
    "backgroundColor": {
      "type": "string"
    }
  }
}