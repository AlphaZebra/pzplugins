{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pz/pz-person-header",
  "version": "0.1.0",
  "title": "PZ Person Header",
  "category": "widgets",
  "icon": "smiley",
  "description": "A card header showing current person record basic info.",
  "supports": {
    "html": false
  },
  "textdomain": "pzdata",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "field": {
      "type": "string"
    },
    "isEdit": {
      "type": "boolean",
      "default": false
    },
    "mustBeNew": {
      "type": "boolean",
      "default": false
    },
    "redirectURL": {
      "type": "string",
      "default": "/"
    }
  }
}