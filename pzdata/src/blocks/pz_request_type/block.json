{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "pz/pz-request-type",
  "version": "0.1.0",
  "title": "PZ Request Type",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "pzdata",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "field": {
      "type": "string"
    },
    "isEdit": {
      "type": "boolean",
      "default": false
    },
    "viewCategory": {
      "type": "boolean",
      "default": false
    },
    "catFilter": {
      "type": "string",
      "default": ""
    },
    "editURL": {
      "type": "string",
      "default": "/"
    },
    "redirectURL": {
      "type": "string",
      "default": "/"
    },
    "prompt": {
      "type": "string"
    }
  }
}
