{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pz/pz-interaction-edit",
	"version": "0.1.0",
	"title": "PZ Interaction Edit",
	"category": "widgets",
	"icon": "smiley",
	"description": "Editing form for individual interactions.",
	"supports": {
		"html": false
	},
	"textdomain": "pzdata",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"field": {
			"type": "string"
		},
		"isEdit": {
			"type": "boolean",
			"default": false
		},
		"listURL": {
			"type": "string",
			"default": "/interactions/"
		},
		"prompt": {
			"type": "string"
		}
	}
}
