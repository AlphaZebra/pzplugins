{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "pz/pz-person-edit",
	"version": "0.1.0",
	"title": "PZ Person Edit",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"supports": {
		"html": false
	},
	"textdomain": "pzdata",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"field": {
			"type": "string"
		},
		"isEdit": {
			"type": "boolean",
			"default": false
		},
		"mustBeNew": {
			"type": "boolean",
			"default": false
		},
		"redirectURL": {
			"type": "string",
			"default": "/"
		}
	}
}
