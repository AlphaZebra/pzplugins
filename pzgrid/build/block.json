{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "pz/pzgrid",
  "version": "0.1.0",
  "title": "PZ Person Grid",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "supports": {
    "html": false
  },
  "textdomain": "pzgrid",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "myheader": {
      "type": "string"
    },
    "siteURL": {
      "type": "string"
    },
    "addURL": {
      "type": "string",
      "default": "/edit-person"
    },
    "editURL": {
      "type": "string",
      "default": "/edit-person"
    },
    "onlyExpired": {
      "type": "boolean",
      "default": "false"
    },
    "queryTail": {
      "type": "string",
      "default": "WHERE id = 15"
    },
    "tag": {
      "type": "string"
    }
  }
}